{% extends 'EvalforGescompevalBundle:Skill:readcompetency.html.twig' %}

{% block title %}
	Gescompeval - {{ 'competency.read.title'|trans({}, 'EvalforGescompevalBundle') }}
{% endblock %}

{% block skill_title %}
	{{ 'competency.read.title'|trans({}, 'EvalforGescompevalBundle') }}
{% endblock %}

{% block form_tag %}
	<form action="{{ path('EGB_update_competency') }}" method="post" role="form">
{% endblock %}

{% block option0 %}
	<option value="0">
		{{ 'competency.select'|trans({}, 'EvalforGescompevalBundle') }}
