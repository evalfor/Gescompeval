{% endblock %}

{% block competencetype %}
	<div class="form-group">
		<div class="row-fluid">
			{{ form_label(form.competencetype) }}
			<br />
			{{ form_errors(form.competencetype) }}
			{{ form_widget(form.competencetype) }}
		</div>
	</div>
{% endblock %}
