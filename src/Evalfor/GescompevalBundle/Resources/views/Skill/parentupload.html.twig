{% extends 'EvalforGescompevalBundle::layout.html.twig' %}

{% block title %}

{% endblock %}

{% block contenido %}
	<div class="panel panel-default">
		<div class="panel-heading">
	    	<h3 class="panel-title">
			{% block skill_title %}

			{% endblock %}
	    	</h3>
	    </div>
	   	<div class="panel-body">
	   		<div class="col-md-6">
		   		{% block form_tag %}
				{% endblock %}
				{{ form_errors(form) }}
			    <div class="form-group">
			   		{{ form_row(form.file) }}				      		
				</div>
				<div class="form-group" style="width:5em;">
			   		{{ form_row(form.delimiter) }}				      		
				</div>
				
				{% block competencetype %}
				{% endblock %}
				
				{{ form_end(form) }}
			</div>
			<div class="col-md-6">
				{% block instructions %}
	
				{% endblock %} 
			</div>
		</div>
	</div>
{% endblock %}