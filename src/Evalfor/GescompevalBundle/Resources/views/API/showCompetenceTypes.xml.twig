<?xml version="1.0" encoding="utf-8"?>
<sr:competencetypes xmlns:sr="showCompetenceTypes"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="showCompetenceTypes showCompetenceTypes.xsd">
	{% for competencetype in competencetypes %}
	<competencetype>
		<id>{{ competencetype.id }}</id>
		<type>{{ competencetype.type }}</type>
		<description>{{ competencetype.description }}</description>
	</competencetype>
	{% endfor %}
</sr:competencetypes>